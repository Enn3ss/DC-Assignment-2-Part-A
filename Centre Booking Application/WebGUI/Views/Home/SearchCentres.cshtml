@{
    @using WebGUI.Models;
    @model IEnumerable<Centre>;
}
<div id="centresResultDiv">
    <p>Search centres result.</p>
    <table>

        @foreach (var centre in Model)
        {
            <tr onclick="test()">
                <td>@centre.Id</td>
                <td>@centre.CentreName</td>
            </tr>
        }
    </table>

</div>
